<plugin>
    <name>ldapAuth</name>
    <creationDate>2014-12-24</creationDate>
    <author>Karen Mikaela</author>
    <authorEmail>karen.mikaela@gmailcom</authorEmail>
    <authorUrl>karen-mikaela.github.io</authorUrl>
